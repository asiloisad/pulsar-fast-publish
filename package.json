{
  "name": "git-publish",
  "description": "Publish package to Github without PPM",
  "repository": "https://github.com/bacadra/pulsar-git-publish",
  "keywords": [
    "publish",
    "github"
  ],
  "version": "1.0.3",
  "license": "MIT",
  "engines": {
    "atom": ">=1.100.0 <2.0.0"
  },
  "main": "./lib/main",
  "activationCommands": {
    ".tree-view .directory": [
      "git-publish:major",
      "git-publish:minor",
      "git-publish:patch"
    ]
  },
  "consumedServices": {
    "tree-view": {
      "versions": {
        "^1.0.0": "consumeTreeView"
      }
    }
  }
}
