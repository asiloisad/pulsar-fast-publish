{
  "name": "package-publish",
  "description": "Publish package to Github without PPM",
  "repository": "https://github.com/asiloisad/pulsar-package-publish",
  "keywords": [
    "publish",
    "github"
  ],
  "version": "1.0.5",
  "license": "MIT",
  "engines": {
    "atom": ">=1.100.0 <2.0.0"
  },
  "main": "./lib/main",
  "activationCommands": {
    ".tree-view .directory": [
      "package-publish:major",
      "package-publish:minor",
      "package-publish:patch",
      "package-publish:major-if",
      "package-publish:minor-if",
      "package-publish:patch-if",
      "ppm-publish:major",
      "ppm-publish:minor",
      "ppm-publish:patch",
      "ppm-publish:major-if",
      "ppm-publish:minor-if",
      "ppm-publish:patch-if"
    ],
    "atom-text-editor:not([mini])": [
      "package-publish:major",
      "package-publish:minor",
      "package-publish:patch",
      "package-publish:major-if",
      "package-publish:minor-if",
      "package-publish:patch-if",
      "ppm-publish:major",
      "ppm-publish:minor",
      "ppm-publish:patch",
      "ppm-publish:major-if",
      "ppm-publish:minor-if",
      "ppm-publish:patch-if"
    ]
  },
  "consumedServices": {
    "tree-view": {
      "versions": {
        "^1.0.0": "consumeTreeView"
      }
    }
  }
}